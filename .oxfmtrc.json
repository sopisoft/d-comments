{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "endOfLine": "lf",
  "experimentalSortImports": {
    "groups": [["side-effect"], ["builtin"], ["external"], ["internal"], ["parent", "sibling", "index"]],
    "sortSideEffects": false,
    "newlinesBetween": false,
    "ignoreCase": true
  }
}
